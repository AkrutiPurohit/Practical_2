//Write a C++ program for setprecision() function.

#include<iostream>
#include<iomanip>

using namespace std;

int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    
    float n;
    cout<<"Enter the floating point number:";
    cin>>n;
    
    cout<<"The value of n:"<<n<<endl;
    cout<<"The value of n:"<<setprecision(3)<<n<<endl;
    return 0;
}
