#include<iostream>

using namespace std;

inline int cube(int a)
{
    return a*a*a;
}

int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    int n;
    cout<<"Enter the number:";
    cin>>n;
    for(int i = 1;i <= n;i++)
    {
        cout<<"The cube of "<<i<<"="<<cube(i)<<endl;
    }
    return 0;
}
