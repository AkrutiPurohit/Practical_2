#include<iostream>

using namespace std;

float area(float r,float pi = 3.14)
{
    return pi*r*r;
}

int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    
    float r;
    cout<<"enter the radius:";
    cin>>r;
    
    cout<<"The area of a circle is:"<<area(r)<<endl;
    return 0;
}
